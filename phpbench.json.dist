{
    "bootstrap": "autoload.php",
    "path": "src/Bench",
    "reports": {
        "bench": {
            "extends": "aggregate",
            "break": ["params"],
            "cols": [ "suite", "benchmark", "mean", "diff", "worst", "best", "mem_real" ],
            "sort": {"mean": "asc"}
        }
    },
    "extensions": [
        "PhpSerializers\\Benchmarks\\BenchExtension"
    ]
}
